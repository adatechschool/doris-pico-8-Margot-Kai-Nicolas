   --mob
function init_mob()
	mob_sp=11
	mob_x=70
	mob_y=60
end

function update_mob()
	mob_sp+=1
	if mob_sp>14 then
	mob_sp=1
	end
end

function draw_mob()
	cls()
	spr(mob_sp,mob_x,mob_y)
end
   
